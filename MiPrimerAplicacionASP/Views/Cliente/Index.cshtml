@model System.Collections.Generic.List<MiPrimerAplicacionASP.Models.ClienteCLS>

@{
    ViewBag.Title = "title";
}

<h2>Clientes</h2>
<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <td>@Html.LabelFor(p=>Model[0].iidcliente)</td>
        <td>@Html.LabelFor(p=>Model[0].nombre)</td>
        <td>@Html.LabelFor(p=>Model[0].appaterno)</td>
        <td>@Html.LabelFor(p=>Model[0].appaterno)</td>
        <td>@Html.LabelFor(p=>Model[0].telefono)</td>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.iidcliente</td>
            <td>@item.nombre</td>
            <td>@item.appaterno</td>
            <td>@item.apmaterno</td>
            <td>@item.telefono</td>
        </tr>
    }
    </tbody>
</table>
